    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="14dp"
        android:background="@drawable/detail_table"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="7.5dp"
            android:background="@drawable/diagno_table_01"
            android:gravity="right">

            <Button
                android:id="@+id/diagno_btn_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="17dp"
                android:layout_marginTop="64dp"
                android:background="@drawable/btn_diagno_ok" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="209.5dp"
            android:layout_marginTop="7.5dp"
            android:background="@drawable/diagno_table_02"
            android:gravity="right">


            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_mileage_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="2dp"
                android:layout_marginTop="75dp"
                android:gravity="right"
                android:text="@string/common_zero"
                android:textColor="@color/_FFFFFF"
                android:textSize="15dp" />

            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="17dp"
                android:layout_marginTop="75dp"
                android:layout_toRightOf="@id/diagno_mileage_text"
                android:text="@string/common_lower_kilometer"
                android:textColor="@color/_FFFFFF"
                android:textSize="15dp" />
        </RelativeLayout>

