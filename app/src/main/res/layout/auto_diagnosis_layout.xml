                android:textSize="15dp" />

            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_time_text3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="17dp"
                android:layout_marginTop="75dp"
                android:text="@string/common_time"
                android:textColor="@color/_FFFFFF"
                android:textSize="15dp" />

        </LinearLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="149dp"
            android:background="@drawable/diagno_table_04"
            android:gravity="right">

            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_oil_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="2dp"
                android:layout_marginTop="68dp"
                android:text="@string/common_zero"
                android:textColor="@color/_FFFFFF"
                android:textSize="24dp" />

            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="17dp"
                android:layout_marginTop="75dp"
                android:layout_toRightOf="@id/diagno_oil_text"
                android:text="@string/common_won"
                android:textColor="@color/_FFFFFF"
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="209.5dp"
            android:layout_marginTop="149dp"
