    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="14dp"
        android:background="@drawable/detail_table"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="7.5dp"
            android:background="@drawable/diagno_table_01"
            android:gravity="right" >
            
            <Button 
                android:id="@+id/diagno_btn_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_diagno_ok"
                android:layout_marginTop="64dp"
                android:layout_marginRight="17dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="209.5dp"
            android:layout_marginTop="7.5dp"
            android:background="@drawable/diagno_table_02"
            android:gravity="right" >
            
             
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_mileage_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="2dp"
                android:gravity="right"
                android:textSize="15dp"
                android:text="0" />
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/diagno_mileage_text"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="17dp"
                android:textSize="15dp"
                android:text="KM" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="409.5dp"
            android:layout_marginTop="7.5dp"
            android:background="@drawable/diagno_table_03"
            android:gravity="right"
            android:orientation="horizontal" >
            
             <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_time_text1"  
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:textSize="15dp"
                android:text="00:" />
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_time_text2"  
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="1dp"
                android:textSize="15dp"
                android:text="00:" />
            
             <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_time_text3"  
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="17dp"
                android:textSize="15dp"
                android:text="00" />
            
        </LinearLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="149dp"
            android:background="@drawable/diagno_table_04"
            android:gravity="right" >
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_oil_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="68dp"
                android:layout_marginRight="2dp"
                android:textSize="24dp"
                android:text="0" />
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/diagno_oil_text"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="17dp"
                android:textSize="15dp"
                android:text="원" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="209.5dp"
            android:layout_marginTop="149dp"
            android:background="@drawable/diagno_table_05"
            android:gravity="right" >
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_coolant_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="2dp"
                android:textSize="15dp"
                android:text="0" />
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/diagno_coolant_text"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="17dp"
                android:textSize="15dp"
                android:text="℃" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="409.5dp"
            android:layout_marginTop="149dp"
            android:background="@drawable/diagno_table_06"
            android:gravity="right" >
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:id="@+id/diagno_fuel_consumption_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="68dp"
                android:layout_marginRight="2dp"
                android:textSize="24dp"
                android:text="0" />
            
            <kr.hdd.carleamingTest.common.textview.NanumBarunGothicBold_TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/diagno_fuel_consumption_text"
                android:textColor="@color/_FFFFFF"
                android:layout_marginTop="75dp"
                android:layout_marginRight="17dp"
                android:textSize="15dp"
                android:text="L" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>